@model Wheather.Models.Three.Three

<div class="weather-list">
    <h2>@Model.City.Name</h2>
    @foreach (var weather in @Model.List)
    {
        var date = new DateTime(1970, 1, 1, 0, 0, 0).AddSeconds(weather.Dt).Date;
        var icon = $"http://openweathermap.org/img/w/{@weather.Weather[0].Icon}.png";
        <div class="weather">
            <h2>@date.ToString("D")</h2>
            <h5>Temperature: @weather.Temp.Min - @weather.Temp.Max C</h5>
            <img src="@icon">
        </div>
    }
</div>